{
  "name": "ideazz-coalition",
  "version": "1.0.0",
  "description": "Just a coalition of ideaz ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist/server/index.js",
    "start:dev": "nodemon --exec babel-node ./server/index.js",
    "build": "babel server -d dist/server",
    "migration:dev:undo": "sequelize db:migrate:undo:all --env development",
    "migration:dev": "sequelize db:migrate --env development",
    "migration:test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "migration:prod": "sequelize db:migrate --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rovilay/ideazz.git"
  },
  "author": "rovilay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rovilay/ideazz/issues"
  },
  "homepage": "https://github.com/rovilay/ideazz#readme",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.8.12"
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "nodemon": "^1.19.1"
  }
}
